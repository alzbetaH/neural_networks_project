{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug xor",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/network",
            "args": [
                "-e", "2000", 
                "-b", "1", 
                "-l", "0.1", 
                "2", 
                "2", 
                "1"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb",
            "miDebuggerArgs": "",
            "logging": {
                "trace": false,
                "traceResponse": false,
                "engineLogging": false,
                "programOutput": false,
                "exceptions": false
            }
        }
    ]
}